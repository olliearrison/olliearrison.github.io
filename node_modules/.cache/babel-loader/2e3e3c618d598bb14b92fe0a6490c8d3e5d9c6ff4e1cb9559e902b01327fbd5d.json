    this.scene.add(plane);\n    },\n    animate() {\n      requestAnimationFrame(this.animate);\n      this.cube.rotation.x += 0.01;\n      this.cube.rotation.y += 0.01;\n      this.renderer.render(this.scene, this.camera);\n    }\n  }\n};\n</script>\n\n<style scoped>\nbody {\n  margin: 0;\n  padding: 0;\n  overflow: hidden; /* Disable scroll */\n  width: 100vw;\n  height: 100vh;\n  display: block;\n}\n\n.three-container {\n  width: 100vw;\n  height: 100vh;\n  margin: 0;\n  padding: 0;\n  overflow: auto;\n  margin-right: 0;\n  margin-left: auto;\n  padding-right: 0;\n  padding-left: 0;\n  display: block;\n}\n\n#app {\n  display: block;\n  width: 100vw;\n  height: 100vh;\n  margin: 0;\n  padding: 0;\n  position: absolute;\n  box-sizing: border-box;\n  overflow: hidden; /* Disable scroll */\n}\n</style>\n"],"mappings":"AAKA,OAAO,KAAKA,KAAI,MAAO,OAAO;AAE9B,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EACDC,IAAIA,CAAA,EAAG,CACP,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;IAChB,IAAI,CAACC,OAAO,CAAC,CAAC;EAChB,CAAC;EACDC,OAAO,EAAE;IACPF,SAASA,CAAA,EAAG;MACV;MACA,IAAI,CAACG,KAAI,GAAI,IAAIV,KAAK,CAACW,KAAK,CAAC,CAAC;;MAE9B;MACA,IAAIC,aAAY,GAAKC,YAAY,CAACC,QAAQ,CAAC,CAAC;MAC5C,IAAIC,cAAa,GAAIF,YAAY,CAACG,SAAS,CAAC,CAAC;MAC7C,IAAIC,cAAa,GAAIL,aAAY,GAAIG,cAAc;MAEnD,IAAIG,MAAK,GAAI,IAAIlB,KAAK,CAACmB,iBAAiB,CAAE,EAAE,EAAEF,cAAc,EAAE,CAAC,EAAE,KAAM,CAAC;MACxE,IAAI,CAACC,MAAM,CAACE,QAAQ,CAACC,CAAA,GAAI,CAAC;;MAE1B;MACA,IAAI,CAACC,QAAO,GAAI,IAAItB,KAAK,CAACuB,aAAa,CAAC,CAAC;MACzC,IAAI,CAACD,QAAQ,CAACE,OAAO,CAAC,IAAI,CAACC,KAAK,CAACC,cAAc,CAACC,WAAW,EAAE,IAAI,CAACF,KAAK,CAACC,cAAc,CAACE,YAAY,CAAC;MACpG,IAAI,CAACH,KAAK,CAACC,cAAc,CAACG,WAAW,CAAC,IAAI,CAACP,QAAQ,CAACQ,UAAU,CAAC;;MAE/D;MACA,MAAMC,QAAO,GAAI,IAAI/B,KAAK,CAACgC,WAAW,CAAC,CAAC;MACxC,MAAMC,QAAO,GAAI,IAAIjC,KAAK,CAACkC,iBAAiB,CAAC;QAAEC,KAAK,EAAE;MAAS,CAAC,CAAC;MACjE,IAAI,CAACC,IAAG,GAAI,IAAIpC,KAAK,CAACqC,IAAI,CAACN,QAAQ,EAAEE,QAAQ,CAAC;MAC9C,IAAI,CAACvB,KAAK,CAAC4B,GAAG,CAAC,IAAI,CAAC